{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Latency (TTFT vs Total)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "llm_total_duration_milliseconds_sum / llm_total_duration_milliseconds_count",
          "legendFormat": "Total latency",
          "refId": "A"
        },
        {
          "expr": "llm_ttft_milliseconds_sum / llm_ttft_milliseconds_count",
          "legendFormat": "TTFT",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Tokens Per Second",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "tokens/s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "llm_tokens_per_second_per_second_sum / llm_tokens_per_second_per_second_count",
          "legendFormat": "TPS",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Generation Duration",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "llm_generation_duration_milliseconds_sum / llm_generation_duration_milliseconds_count",
          "legendFormat": "Generation time",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Token Counts",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "llm_prompt_tokens_total",
          "legendFormat": "Prompt tokens",
          "refId": "A"
        },
        {
          "expr": "llm_completion_tokens_total",
          "legendFormat": "Completion tokens",
          "refId": "B"
        }
      ]
    }
  ],
  "refresh": "5s",
  "schemaVersion": 39,
  "tags": ["llm", "observability"],
  "templating": { "list": [] },
  "time": { "from": "now-30m", "to": "now" },
  "title": "LLM Metrics",
  "uid": "llm-metrics"
}